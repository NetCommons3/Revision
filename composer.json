{
    "name": "netcommons/revision",
    "description": "Revision Plugin for Cakephp",
    "homepage": "http://www.netcommons.org/",
    "require": {
        "cakephp/cakephp":        "~2.4",
        "cakephp/debug_kit":      "~2.2",
        "cakedc/migrations":      "~2.2",
        "twbs/bootstrap":         "~3.1",
        "components/jquery":      "~2.1"
    },
    "require-dev": {
        "satooshi/php-coveralls": "dev-master"
    },
    "license": "NetCommons License",
    "authors": [
        {
            "name": "NetCommons Community",
            "homepage": "https://github.com/NetCommons3/Revision/graphs/contributors"
        }
    ],
    "minimum-stability": "dev",
    "type":        "cakephp-plugin",
    "keywords":    ["cakephp", "revision"],
    "config": {
        "vendor-dir": "vendors"
    }
}
